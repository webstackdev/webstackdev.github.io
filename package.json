{
  "name": "resume-kevin-brown",
  "version": "1.0.0",
  "description": "Resume generator for GitHub Personal Page for Kevin Brown",
  "main": "index.html",
  "author": "kevin@webstackbuilders.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "yarn clean && mkdirp docs && mkdirp .cache",
    "build": "yarn prebuild && yarn build:eleventy && yarn build:sass",
    "build:eleventy": "npx @11ty/eleventy",
    "build:pdf": "node scripts/html-to-pdf.mjs",
    "build:sass": "sass src/assets/scss/index.scss:docs/index.css && sass src/assets/scss/pdf.scss:.cache/pdf.css",
    "clean": "npx rimraf docs && npx rimraf .cache",
    "lint": "prebuild && yarn build && yarn lint:html",
    "lint:html": "npx gulp lint:html",
    "start": "npm-run-all prebuild build:sass --parallel watch:*",
    "test": "jest",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "watch:sass": "sass --watch src/assets/scss/index.scss:docs/index.css"
  },
  "devDependencies": {
    "@11ty/eleventy": "2.0.1",
    "@babel/core": "7.22.9",
    "@babel/preset-env": "7.22.9",
    "@babel/register": "7.22.5",
    "@testing-library/dom": "9.3.1",
    "@types/eslint": "8.44.2",
    "@types/gulp-htmlmin": "1.3.33",
    "babel-eslint": "10.1.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-jest-dom": "5.0.2",
    "eslint-plugin-jsdoc": "46.4.6",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-security": "1.7.1",
    "eslint-plugin-testing-library": "5.11.1",
    "gulp": "4.0.2",
    "gulp-eslint-new": "1.8.3",
    "gulp-htmlmin": "5.0.1",
    "hint": "7.1.10",
    "jest": "29.6.2",
    "jest-axe": "8.0.0",
    "jest-environment-jsdom": "29.6.2",
    "jest-environment-node": "29.6.2",
    "jsdom": "22.1.0",
    "jsdom-testing-mocks": "1.9.0",
    "mkdirp": "3.0.1",
    "prettier": "3.0.1",
    "puppeteer": "21.0.1",
    "rimraf": "5.0.1",
    "typescript": "5.1.6"
  },
  "dependencies": {
    "npm-run-all": "4.1.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}